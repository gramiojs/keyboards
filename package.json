{
	"name": "@gramio/keyboards",
	"version": "1.2.0",
	"description": "Framework-agnostic Telegram bot keyboard builder with many cool features!",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		}
	},
	"type": "module",
	"homepage": "https://github.com/gramiojs/keyboards",
	"readme": "https://gramio.dev/keyboards/overview",
	"scripts": {
		"prepublishOnly": "bunx tsc --noEmit && bun test && bunx pkgroll"
	},
	"keywords": [
		"gramio",
		"telegram",
		"telegram-keyboard",
		"keyboard",
		"inline-keyboard",
		"keyboard-builder",
		"telegram-bot-api",
		"grammy",
		"telegraf",
		"node-telegram-bot-api",
		"puregram",
		"inline-query"
	],
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "^1.1.14",
		"pkgroll": "^2.6.0",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@gramio/types": "^8.2.0"
	},
	"license": "MIT",
	"files": ["dist"]
}
