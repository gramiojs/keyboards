{
	"name": "@gramio/keyboards",
	"version": "1.0.1",
	"description": "Framework-agnostic Telegram bot keyboard builder with many cool features!",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		}
	},
	"type": "module",
	"homepage": "https://github.com/gramiojs/keyboards",
	"readme": "https://gramio.dev/keyboards/overview",
	"scripts": {
		"prepublishOnly": "bun test && bunx pkgroll",
		"jsr": "bun scripts/release-jsr.ts"
	},
	"keywords": [
		"gramio",
		"telegram",
		"telegram-keyboard",
		"keyboard",
		"inline-keyboard",
		"keyboard-builder",
		"telegram-bot-api",
		"grammy",
		"telegraf",
		"node-telegram-bot-api",
		"puregram",
		"inline-query"
	],
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@types/bun": "^1.1.12",
		"pkgroll": "^2.5.1",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"@gramio/types": "*"
	},
	"license": "MIT",
	"files": ["dist"]
}
